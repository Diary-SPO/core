{
  "name": "dnevnik-ts",
  "private": true,
  "version": "7.0.0",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --env-mode development",
    "build": "rsbuild build",
    "preview": "rsbuild preview --env-mode production",
    "serve": "yarn build && yarn preview",
    "predeploy": "yarn build",
    "deploy:gh": "gh-pages -d dist",
    "deploy:vk": "vk-miniapps-deploy --env-mode production",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "biome lint . --apply",
    "format": "biome format . --write",
    "checkAll": "biome check . --apply-unsafe"
  },
  "dependencies": {
    "@vkontakte/icons": "2.95.0",
    "@vkontakte/vk-bridge": "^2.13.0",
    "@vkontakte/vk-bridge-react": "^1.0.0",
    "@vkontakte/vk-mini-apps-router": "1.4.2",
    "@vkontakte/vkui": "6.0.0",
    "preact": "^10.19.3",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@diary-spo/shared": "1.8.9",
    "@diary-spo/types": "latest",
    "@preact/preset-vite": "^2.8.1",
    "@rsbuild/core": "^0.3.8",
    "@rsbuild/plugin-preact": "^0.3.8",
    "@size-limit/preset-app": "^11.0.2",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "@vkontakte/vk-miniapps-deploy": "^0.1.6",
    "gh-pages": "^6.0.0",
    "jsdom": "^23.2.0",
    "size-limit": "^11.0.2",
    "typescript": "^5.2.2",
    "vitest": "^1.2.1",
    "vitest-localstorage-mock": "^0.0.1"
  }
}
